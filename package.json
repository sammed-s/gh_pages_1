{
  "name": "drillbit-analysis-page-frontend",
  "private": true,
  "version": "2.0.0",
  "homepage": "https://sammed-s.github.io/react-gh-pages",
  "type": "module",
  "scripts": {
    "build:pro": "vite build",
    "build": "env-cmd -f env/.env.production vite build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "start": "env-cmd -f env/.env.local vite",
    "start:staging": "env-cmd -f env/.env.development vite --mode development",
    "build:staging": "env-cmd -f env/.env.development vite build",
    "start:prod": "env-cmd -f env/.env.production vite --mode production",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.21",
    "@mui/material": "^5.15.20",
    "@mui/x-charts": "^7.8.0",
    "@reduxjs/toolkit": "^2.2.6",
    "apexcharts": "^3.49.2",
    "axios": "^1.7.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-draggable": "^4.4.6",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.24.0",
    "react-spinners": "^0.12.0",
    "react-toastify": "^10.0.5",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "styled-components": "^6.1.11",
    "vite-plugin-html": "^3.2.2"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "gh-pages": "^6.1.1",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "vite": "^5.3.1"
  }
}
